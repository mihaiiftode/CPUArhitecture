            <TextBox x:Name="ALUTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Controller.CurrentMicroInstruction.ALUOperationString}" VerticalAlignment="Top" Width="72" Margin="242,0,0,0" IsReadOnly="True"/>
            <TextBox x:Name="RBUSTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Controller.CurrentMicroInstruction.RBUSDestinationString}" VerticalAlignment="Top" Width="72" Margin="313,0,0,0" IsReadOnly="True"/>
            <TextBox x:Name="OtherOperationTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Controller.CurrentMicroInstruction.OtherOperationsString}" VerticalAlignment="Top" Width="72" Margin="383,0,0,0" IsReadOnly="True"/>
            <TextBox x:Name="MemoryTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Controller.CurrentMicroInstruction.MemoryOperationsString}" VerticalAlignment="Top" Width="72" Margin="454,0,0,0" IsReadOnly="True"/>
            <TextBox x:Name="JumpTextBox" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Controller.CurrentMicroInstruction.RamificationConditionString}" VerticalAlignment="Top" Width="325" Margin="525,0,0,0" IsReadOnly="True"/>
            <Button x:Name="StepButton" Content="Step" HorizontalAlignment="Left" Margin="0,3,0,0" VerticalAlignment="Top" Width="39" Click="StepButton_Click" />
            <Button x:Name="RunBUtton" Content="Run" HorizontalAlignment="Left" Margin="53,3,0,0" VerticalAlignment="Top" Width="39" Click="RunBUtton_Click" />
            <TextBox x:Name="Index" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="{Binding Controller.CurrentIndex}" VerticalAlignment="Top" Width="45" Margin="911,3,0,0" IsReadOnly="True"/>


        </Grid>
        <Menu DockPanel.Dock="Top" x:Name="MenuBar" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="956" Grid.ColumnSpan="2">
            <MenuItem Header="_File">
                <MenuItem Name="AsmLoadMenuItem" Header="Incarca ASM"  Click="AsmLoadMenuItem_OnClick"/>
                <MenuItem Name="MicroCodeLoadMenuItem" Header="Incarca Microcod" Click="MicroCodeLoadMenuItem_OnClick" />
                <MenuItem Name="ExistMenuItem" Header="Exit" Click="ExistMenuItem_OnClick" />
            </MenuItem>
            <MenuItem Header="_Ferestre">
                <MenuItem Name="ShowFilesMenuItem" Header="Continut fisiere" Click="ShowFilesMenuItem_OnClick"/>
                <MenuItem Name="ShowMicroCodeMenuItem" Header="Microcod" Click="ShowMicroCodeMenuItem_OnClick"/>
                <MenuItem Name="ShowMemeoryMenuItem" Header="Memorie Principala" Click="ShowMemeoryMenuItem_OnClick"/>
                <MenuItem Name="ShowMicroCodeMemoryMenuItem" Header="Memorie MicroProgram" Click="ShowMicroCodeMemoryMenuItem_OnClick"/>
            </MenuItem>
        </Menu>

    </Grid>
</Window>
